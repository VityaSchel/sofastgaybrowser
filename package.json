{
  "name": "sofastgaybrowser",
  "version": "1.0.0",
  "description": "So fast gay browser!",
  "main": "dist/main/index.js",
  "author": "Viktor Shchelochkov <hi@hloth.dev> (https://hloth.dev)",
  "homepage": "https://github.com/vityaschel/sofastgaybrowser",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "watch": "nodemon",
    "build": "electron build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "start": "electron-vite preview --outDir=dist",
    "dev": "electron-vite dev --outDir=dist",
    "package": "electron-vite build --outDir=dist && electron-forge package",
    "make": "electron-vite build --outDir=dist && electron-forge make"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "classnames": "^2.3.2",
    "electron-log": "^5.0.1",
    "electron-nightly": "29.0.0-nightly.20231115",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "gayporn": "^1.1.8",
    "lodash": "^4.17.21",
    "lucide-react": "^0.292.0",
    "node-html-parser": "^6.1.11",
    "react-hotkeys-hook": "^4.4.1",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.19.0",
    "react-use": "^17.4.0",
    "react-use-size": "^3.0.2",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/tmp": "^0.2.6",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-vite": "^1.0.29",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
